<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.lostandfound.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.lostandfound.entity.User">
        <id column="id" property="id" />
        <result column="nickname" property="nickname" />
        <result column="password" property="password" />
        <result column="real_name" property="realName" />
        <result column="age" property="age" />
        <result column="clazz" property="clazz" />
        <result column="phone_number" property="phoneNumber" />
        <result column="gender" property="gender" />
        <result column="email" property="email" />
        <result column="other_contacts" property="otherContacts" />
        <result column="find_num" property="findNum" />
        <result column="lost_num" property="lostNum" />
        <result column="is_deleted" property="isDeleted" />
        <result column="status" property="status" />
        <result column="integral" property="integral" />
        <result column="created_time" property="createdTime" />
        <result column="updated_time" property="updatedTime" />
    </resultMap>

</mapper>
